<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>CheckLoginServlet</servlet-name>
        <servlet-class>login.CheckLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TicketForm</servlet-name>
        <servlet-class>user.TicketForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessTicketForm</servlet-name>
        <servlet-class>user.ProcessTicketForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GenerateBarcodeServlet</servlet-name>
        <servlet-class>user.GenerateBarcodeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerifyCodeServlet</servlet-name>
        <servlet-class>user.VerifyCodeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValidateDispatch</servlet-name>
        <servlet-class>user.ValidateDispatch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RedirectUser</servlet-name>
        <servlet-class>login.RedirectUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetReport</servlet-name>
        <servlet-class>admin.AdminReport</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminHome</servlet-name>
        <servlet-class>admin.AdminHome</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckLoginServlet</servlet-name>
        <url-pattern>/CheckLoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TicketForm</servlet-name>
        <url-pattern>/user/TicketForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessTicketForm</servlet-name>
        <url-pattern>/user/ProcessTicketForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GenerateBarcodeServlet</servlet-name>
        <url-pattern>/GenerateBarcodeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerifyCodeServlet</servlet-name>
        <url-pattern>/user/VerifyCodeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValidateDispatch</servlet-name>
        <url-pattern>/user/ValidateDispatch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RedirectUser</servlet-name>
        <url-pattern>/RedirectUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetReport</servlet-name>
        <url-pattern>/GetReport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminHome</servlet-name>
        <url-pattern>/AdminHome</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
